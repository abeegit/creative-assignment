<div id="carousel" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <% for(var i = 0; i < weather.length; i++) { %>
      <div class="carousel-item <%= i === 0 ? 'active': '' %>"></div>
        <h2><%= weather[i].date %></h2>
        <div class="row" id="hourly">
          <% for(var j = 0; j < weather[i].hourly.length; j++) { %>
            <% var hour = weather[i].hourly[j]; %>
            <div class="col-md-12"><h3><%= hour.time %></h3></div>
            <div class="col">
              <ul>
                <li>Temp: <%= hour.temp %></li>
                <li>Wind: <%= hour.wind %></li>
                <li>Desc: <%= hour.weatherDesc %></li>
                <li>Precipitation: <%= hour.precipitation %></li>
                <li>Chance Of rain: <%= hour.chanceOfRain %></li>
                <li>Chance Of sunshine: <%= hour.chanceOfSunshine %></li>
              </ul>
            </div>
          <% } %>
        </div>
        <div class="row" id="astronomy">
          <div class="col-md-12"><h3>Astronomy</h3></div>
          <div class="col">
            Sunrise<br><strong><%= weather[i].astronomy.sunrise %></strong>
          </div>
          <div class="col">
            Sunset<br><strong><%= weather[i].astronomy.sunset %></strong>
          </div>
          <div class="col">
            Moonrise<br><strong><%= weather[i].astronomy.moonrise %></strong>
          </div>
          <div class="col">
            Moonset<br><strong><%= weather[i].astronomy.moonset %></strong> 
          </div>
        </div>
      </div>
    <% } %>
  </div>
  <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>